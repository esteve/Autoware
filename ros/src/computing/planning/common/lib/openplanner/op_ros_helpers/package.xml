<?xml version="1.0"?>
<package format="2">
    <name>op_ros_helpers</name>
    <version>1.9.1</version>
    <description>Helper functions for OpenPlanner visualization package</description>
    <maintainer email="yusuke.fujii@tier4.jp">Yusuke FUJII</maintainer>
    <author email="hatem.darweesh@g.sp.m.is.nagoya-u.ac.jp">Hatem Darweesh</author>
    <license>BSD</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>autoware_build_flags</buildtool_depend>

    <build_depend>roscpp</build_depend>
    <build_depend>geometry_msgs</build_depend>
    <build_depend>autoware_msgs</build_depend>
    <build_depend>map_file</build_depend>
    <build_depend>pcl_conversions</build_depend>
    <build_depend>pcl_ros</build_depend>
    <build_depend>sensor_msgs</build_depend>
    <build_depend>tf</build_depend>
    <build_depend>jsk_recognition_msgs</build_depend>
    <build_depend>op_utility</build_depend>
    <build_depend>op_planner</build_depend>
    <build_depend>op_simu</build_depend>
    <build_depend>waypoint_follower</build_depend>
    <build_depend>vector_map_msgs</build_depend>

    <exec_depend>roscpp</exec_depend>
    <exec_depend>geometry_msgs</exec_depend>
    <exec_depend>autoware_msgs</exec_depend>
    <exec_depend>map_file</exec_depend>
    <exec_depend>pcl_conversions</exec_depend>
    <exec_depend>pcl_ros</exec_depend>
    <exec_depend>sensor_msgs</exec_depend>
    <exec_depend>tf</exec_depend>
    <exec_depend>jsk_recognition_msgs</exec_depend>
    <exec_depend>op_utility</exec_depend>
    <exec_depend>op_planner</exec_depend>
    <exec_depend>op_simu</exec_depend>
    <exec_depend>waypoint_follower</exec_depend>
    <exec_depend>vector_map_msgs</exec_depend>

    <export>
    <build_type>ament_cmake</build_type>
    </export>
</package>
