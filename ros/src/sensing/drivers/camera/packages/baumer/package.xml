<package format="2">
  <name>vlg22c_cam</name>
  <version>1.9.1</version>
  <description>A ROS Driver for VLG-22C Camera</description>

  <maintainer email="abrahammonrroy@yahoo.com">amc-nu</maintainer>
  <author email="abrahammonrroy@yahoo.com">amc-nu</author>

  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>autoware_build_flags</buildtool_depend>

  <build_depend>image_transport</build_depend> 
  <build_depend>roscpp</build_depend> 
  <build_depend>std_msgs</build_depend> 
  <build_depend>sensor_msgs</build_depend> 
  <build_depend>camera_info_manager</build_depend>

  <exec_depend>image_transport</exec_depend> 
  <exec_depend>roscpp</exec_depend> 
  <exec_depend>std_msgs</exec_depend> 
  <exec_depend>sensor_msgs</exec_depend> 
  <exec_depend>camera_info_manager</exec_depend>

</package>
