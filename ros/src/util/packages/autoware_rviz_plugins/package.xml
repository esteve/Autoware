<?xml version="1.0"?>
<package format="2">
    <name>autoware_rviz_plugins</name>
    <version>1.9.1</version>
    <description>The autoware_rviz_plugins package</description>

    <maintainer email="yusuke.fujii@tier4.jp">Yusuke FUJII</maintainer>
    <author>Yusuke FUJII</author>
    <author>h_ohta</author>

    <license>BSD</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>autoware_build_flags</buildtool_depend>
    <build_depend>rviz</build_depend>
    <build_depend>std_msgs</build_depend>
    <build_depend>qtbase5-dev</build_depend>

    <exec_depend>rviz</exec_depend>
    <exec_depend>std_msgs</exec_depend>
    <exec_depend>libqt5-core</exec_depend>
    <exec_depend>libqt5-widgets</exec_depend>


    <!-- The export tag contains other, unspecified, tags -->
    <export>
    <build_type>ament_cmake</build_type>
        <rviz plugin="${prefix}/plugin_description.xml"/>
    </export>
</package>
